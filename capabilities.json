  },
  "dataViewMappings": [
    {
      "conditions": [
        {
          "NodeId": {
            "max": 1
          },
          "TargetId": {
            "max": 1
          },
          "NodeLabel": {
            "max": 1
          },
          "NodeShape": {
            "max": 1
          }
        }
      ],
      "categorical": {
        "categories": {
          "select": [
            {
              "for": {
                "in": "NodeId"
              }
            },
            {
              "for": {
                "in": "TargetId"
              }
            },
            {
              "for": {
                "in": "NodeMainLabel"
              }
            },
            {
              "for": {
                "in": "NodeSecondLabel"
              }
            },
            {
              "for": {
                "in": "NodeShape"
              }
            },
            {
              "for": {
                "in": "EdgeLabel"
              }
            },
            {
              "for": {
                "in": "NodeTopLabel"
              }
            }
          ],
          "dataReductionAlgorithm": {
            "top": {
              "count": 1000
            }
          }
        }
      }
    }
  ],
  "privileges": []
}
